package maxes;

public class MaxesOfArray {

	public static void main(String[] args) {

		int[] arr1 = new int[4];
		arr1 [0] = 2;
		arr1 [1] = 3;
		arr1 [2] = 4;
		arr1 [3] = 5;
		
		int[] arr2 = new int[4];
		arr2 [0] = 2;
		arr2 [1] = 4;
		
		int[] arr3 = getMaxesOfArray(arr1, arr2); 
		for (int i : arr3) {
			System.out.println(i);
		}
	}

	private static int[] getMaxesOfArray(int[] arr1, int[] arr2) {
		int[] arr = null; 
		int max = getMax(arr2);
		return arr;
	}

	private static int getMax(int[] arr2) {
		
		int max = 0;
	
		
		return max;
		
	}

}
