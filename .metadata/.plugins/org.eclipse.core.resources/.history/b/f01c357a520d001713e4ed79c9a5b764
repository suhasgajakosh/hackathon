package maxArray;

public class MaxesArray {

	public static void main(String[] args) {

		int nums[] = new int[4];
		int maxes[] = new int[2];
		
		nums[0] = 2;
		nums[1] = 3;
		nums[2] = 4;
		nums[3] = 5;
		
		maxes[0] = 2;
		maxes[1] = 4;
		
		int arr[] = getMaxesOfArrays(nums, maxes);
		for (int i : arr) {
			System.out.println(i);
		}
		
	}

	private static int[] getMaxesOfArrays(int[] nums, int[] maxes) {
		int size = 0;
		if (nums.length > maxes.length) {
			size = nums.length;
		} else {
			size = maxes.length;
		}
		int arr[] = new int[size];
		int max = getMaxinMaxes(maxes);
		return arr;
	}

	private static int getMaxinMaxes(int[] maxes) {
		int max = 0;
		return max;
	}

}
